end)

--//Functions - Functions//--
FH_Hold.F_EditM4A1.MouseButton1Click:Connect(function()
	singleClick(FH_Hold.F_EditM4A1)
	if game.Players.LocalPlayer.Backpack:FindFirstChild('M4A1') then
		local player = game:GetService("Players").LocalPlayer
		local gun = player.Backpack:FindFirstChild("M4A1")
		local GunStats = require(gun:FindFirstChild("GunStates"))
		GunStats["Damage"] = math.huge
		GunStats["MaxAmmo"] = math.huge
		GunStats["StoredAmmo"] = math.huge
		GunStats["CurrentAmmo"] = math.huge
		GunStats["FireRate"] = 0.02
		GunStats["Range"] = math.huge
		GunStats["ReloadTime"] = 0.02
		GunStats["Bullets"] = 10
		GunStats["Spread"] = 1
	else
		print('PrisonHx - [EditM4A1]: Weapon needs to be in backpack and not equipped!!!')
		warn('PrisonHx - [EditM4A1]: Weapon needs to be in backpack and not equipped!!!')
		error('PrisonHx - [EditM4A1]: Weapon needs to be in backpack and not equipped!!!')
	end
end)

