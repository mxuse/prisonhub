local PirsonHx = game.CoreGui.PirsonHx
local TeleportsHolder = PirsonHx.TeleportsHolder
local TH_Hold = TeleportsHolder.TH_Hold
local FunctionsHolder = PirsonHx.FunctionsHolder
local FH_Hold = FunctionsHolder.FH_Hold
local CombatHolder = PirsonHx.CombatHolder
local CB_Hold = CombatHolder.CB_Hold
local LocalPlayerHolder = PirsonHx.LocalPlayerHolder
local LP_Hold = LocalPlayerHolder.LP_Hold
local BlurEffect = Instance.new('BlurEffect', game.Lighting)

--//Variables//--
local UI_Open = true
local UI_Elements = {TeleportsHolder, FunctionsHolder, CombatHolder, LocalPlayerHolder}
local ipit = game:GetService("UserInputService")
game.Lighting.Blur.Enabled = true
pcall(function() local starterGui = game:GetService('StarterGui') starterGui:SetCore("TopbarEnabled", false) end)

--//Other - Functions//--
TeleportsHolder.Draggable = true
FunctionsHolder.Draggable = true
CombatHolder.Draggable = true
LocalPlayerHolder.Draggable = true
ipit.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.RightShift then
		if UI_Open == true then
			for i, v in pairs(UI_Elements) do
				if v.ClassName == "TextLabel" then
					v.Visible = false
				end
			end
			game.Lighting.Blur.Enabled = false
			pcall(function() local starterGui = game:GetService('StarterGui') starterGui:SetCore("TopbarEnabled", true) end)
			PirsonHx.UIName.Visible = false
			PirsonHx.UIVersion.Visible = false
			wait(.2)
			UI_Open = false
		elseif UI_Open == false then
			for i, v in pairs(UI_Elements) do
				if v.ClassName == "TextLabel" then
					v.Visible = true
				end
			end
			game.Lighting.Blur.Enabled = true
			pcall(function() local starterGui = game:GetService('StarterGui') starterGui:SetCore("TopbarEnabled", false) end)
			PirsonHx.UIName.Visible = true
			PirsonHx.UIVersion.Visible = true
			wait(.2)
			UI_Open = true
		end
    end
end)

function singleClick(btn)
	btn.TextColor3 = Color3.new(0, 255, 0)
	wait(.3)
	btn.TextColor3 = Color3.new(255,255,255)
end
function longClick(btn)
	btn.TextColor3 = Color3.new(0, 255, 0)
end

--//Color - Functions//--
function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
counter = 0 counter1 = 0
local TH_Color = coroutine.wrap(function() while wait()do TeleportsHolder.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1) counter = counter + 0.005 end end)
local FH_Color = coroutine.wrap(function() while wait()do FunctionsHolder.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)  end end)
local CB_Color = coroutine.wrap(function() while wait()do CombatHolder.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)  end end)
local LP_Color = coroutine.wrap(function() while wait()do LocalPlayerHolder.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)  end end)
local N_Color = coroutine.wrap(function() while wait()do PirsonHx.UIName.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)  end end)
TH_Color()
FH_Color()
CB_Color()
LP_Color()
N_Color()

--//Functions - Combat//--
CB_Hold.C_ClickArrest.MouseButton1Click:Connect(function()
	longClick(CB_Hold.C_ClickArrest)
	
	local mouse = game.Players.LocalPlayer:GetMouse()
	local arrestEvent = game.Workspace.Remote.arrest
	mouse.Button1Down:connect(function()
		local obj = mouse.Target
		local response = arrestEvent:InvokeServer(obj)
	end)
end)
CB_Hold.C_KillAura.MouseButton1Click:Connect(function()
	singleClick(CB_Hold.C_KillAura)
	
	mainRemotes = game.ReplicatedStorage
	meleeRemote = mainRemotes['meleeEvent']
	killAura = false
	contextactionservice = game.ContextActionService
	function toggleKillAura(actionName, inputState, inputObject)
		print('Doing the action : ' .. actionName)
		if inputState == Enum.UserInputState.Begin then
			if killAura == true then
				killAura = false
			else
				killAura = true
			end
		end
	end
	contextactionservice:BindAction('ToggleKillAura', toggleKillAura, false, Enum.KeyCode.K)
	while wait() do
		if killAura == true then
			for _, plr in pairs (game:GetService('Players'):GetChildren()) do
				if plr.Name ~= game.Players.LocalPlayer.Name then
					meleeRemote:FireServer(plr)
				end
			end
		end
	end
end)
CB_Hold.C_ArrestAll.MouseButton1Click:Connect(function()
	singleClick(CB_Hold.C_ArrestAll)
	
	local Player = game.Players.LocalPlayer
	local cpos = Player.Character.HumanoidRootPart.CFrame
	for i,v in pairs(game.Teams.Criminals:GetPlayers()) do
		if v.Name ~= Player.Name then
			local i = 10
			repeat
				wait()
				i = i-1
				game.Workspace.Remote.arrest:InvokeServer(v.Character.HumanoidRootPart)
				Player.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 1)
			until i == 0
		end
	end
end)

CB_Hold.C_Bypass.MouseButton1Click:Connect(function()
	singleClick(CB_Hold.C_Bypass)
	
	game.Players.LocalPlayer.CharacterAdded:connect(function()
  		game.Workspace:WaitForChild(game.Players.LocalPlayer.Name)
  		game.Players.LocalPlayer.Character.ClientInputHandler.Disabled = true
	end)
end)

--//Functions - LocalPlayer//--
_G.GodMode = false
LP_Hold.L_Godmode.MouseButton1Click:Connect(function()
	if _G.GodMode == false then
		lplayer = game:GetService("Players").LocalPlayer
        lplayer.Character.Humanoid.Name = 1
        local l = lplayer.Character["1"]:Clone()
        l.Parent = lplayer.Character
        l.Name = "Humanoid"
        wait(0.1)
        lplayer.Character["1"]:Destroy()
        game:GetService("Workspace").CurrentCamera.CameraSubject = lplayer.Character
        lplayer.Character.Animate.Disabled = true
        wait(0.1)
        lplayer.Character.Animate.Disabled             = false
        lplayer.Character.Humanoid.DisplayDistanceType = "None"
		wait(.5)
		LP_Hold.L_Godmode.TextColor3 = Color3.new(0, 255, 0)
		_G.GodMode = true
	elseif _G.GodMode == true then
		lplayer.Character.Humanoid.Health = 0
		local a1 = Instance.new("Model", workspace)
		local a2 = Instance.new("Part", a1)
		a2.CanCollide = true
		a2.Anchored = true
		a2.CFrame = CFrame.new(10000, 10000, 10000)
		a2.Name = "Torso"
		local a3 = Instance.new("Humanoid", a1)
		a3.MaxHealth=100;a3.Health=100
		game.Players.LocalPlayer.Character = a1
		a3.Health=0
		wait(.5)
		LP_Hold.L_Godmode.TextColor3 = Color3.new(255, 255, 255)
		_G.GodMode = false
	end
end)

LP_Hold.L_JumpPower.MouseButton1Click:Connect(function()
	singleClick(LP_Hold.L_JumpPower)
	if LP_Hold.L_JumpPower.JPInput.Text ~= "" or LP_Hold.L_JumpPower.JPInput.Text ~= " " then
		LP_Hold.L_JumpPower.JPInput.Text = "100"
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = tonumber(LP_Hold.L_JumpPower.JPInput.Text)
	else
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = tonumber(LP_Hold.L_JumpPower.JPInput.Text)
	end
end)

LP_Hold.L_NoClip.MouseButton1Click:Connect(function()
	singleClick(LP_Hold.L_NoClip)
	game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end)

LP_Hold.L_WalkSpeed.MouseButton1Click:Connect(function()
	singleClick(LP_Hold.L_WalkSpeed)
	if LP_Hold.L_WalkSpeed.WSInput.Text ~= "" or LP_Hold.L_WalkSpeed.WSInput.Text ~= " " then
		LP_Hold.L_WalkSpeed.WSInput.Text = "100"
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = tonumber(LP_Hold.L_WalkSpeed.WSInput.Text)
	else
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = tonumber(LP_Hold.L_WalkSpeed.WSInput.Text)
	end
end)

